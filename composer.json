{
    "name": "sanpi/assetic-bundle",
    "description": "Integrates Assetic into Symfony2",
    "keywords": ["assets", "compression", "minification"],
    "homepage": "https://github.com/sanpii/AsseticBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kris Wallsmith",
            "email": "kris.wallsmith@gmail.com",
            "homepage": "http://kriswallsmith.net/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/asset": "~2.3|~3.0|~4.0",
        "symfony/framework-bundle": "~2.3|~3.0|~4.0",
        "symfony/dependency-injection": "~2.3|~3.0|~4.0",
        "symfony/console": "~2.3|~3.0|~4.0",
        "symfony/http-kernel": "~2.3|~3.0|~4.0",
        "symfony/templating": "~2.3|~3.0|~4.0",
        "symfony/yaml": "~2.3|~3.0|~4.0",
        "sanpi/assetic": "^1.6"
    },
    "require-dev": {
        "patchwork/jsqueeze": "~1.0",
        "kriswallsmith/spork": "~0.3",
        "phpunit/phpunit": "~7.0",
        "symfony/twig-bundle": "~2.3|~3.0|~4.0",
        "symfony/dom-crawler": "~2.3|~3.0|~4.0",
        "symfony/css-selector": "~2.3|~3.0|~4.0",
        "symfony/phpunit-bridge": "~2.7|~3.0|~4.0"
    },
    "replace": {
        "symfony/assetic-bundle": "self.version"
    },
    "conflict": {
        "twig/twig": "<1.34|>=2,<2.4",
        "kriswallsmith/spork": "<=0.2"
    },
    "suggest": {
        "symfony/twig-bundle": "to use the Twig integration",
        "kriswallsmith/spork": "to be able to dump assets in parallel"
    },
    "autoload": {
        "psr-4": { "Symfony\\Bundle\\AsseticBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
